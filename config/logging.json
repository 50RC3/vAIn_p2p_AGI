{
    "level": "INFO",
    "format": "%(asctime)s | %(name)s | %(levelname)s | [%(filename)s:%(lineno)d] | %(message)s",
    "file_logging": true,
    "max_bytes": 10485760,
    "backup_count": 5,
    "handlers": {
        "console": {
            "enabled": true,
            "level": "INFO"
        },
        "file": {
            "enabled": true,
            "level": "DEBUG"
        }
    }
}
